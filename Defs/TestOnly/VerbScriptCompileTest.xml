<Defs>
  <VerbScript.VerbScriptDef>
    <defName>VSC_SimpleTest_AA</defName>
	<verbData>
		<potential>
			<and>
				<isGender>Male</isGender>
				<not><hasHediff>Dementia</hasHediff></not>
			</and>
		</potential>
		<allow>
		</allow>
		<init>
		</init>
		<allowTarget>
		</allowTarget>
		<ai_activeTargets>
			<hostilepawnsinmap></hostilepawnsinmap>
		</ai_activeTargets>
		<ai_targetPoints>
