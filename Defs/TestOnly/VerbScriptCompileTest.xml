  <VerbScript.VerbScriptDef>
    <defName>VSC_SimpleTest_BA</defName>
	<verbData>
		<potential>
			<and>
				<isGender>Male</isGender>
				<not><hasHediff>Dementia</hasHediff></not>
			</and>
		</potential>
		<allow>
		</allow>
		<init>
		</init>
		<allowTarget>
		</allowTarget>
		<ai_activeTargets>
			<hostilepawnsinmap></hostilepawnsinmap>
		</ai_activeTargets>
		<ai_targetPoints>
			<rand>
				<num>1</num>
				<num>2</num>
			</rand>
		</ai_targetPoints>
		<ai_targetMode>
		</ai_targetMode>
		<fire>
			<save>
				<string>SpinVector</string>
				<mult>
					<normalize>
						<sub>
							<aimtarget/>
							<topscope/>
						</sub>
					</normalize>
					<num>5</num>
				</mult>
			</save>
			<save>
				<string>Counter</string>
				<num>0</num>
			</save>
			<while>
				<less>
					<load>Counter</load>
					<num>40</num>
				</less>
				<save>
					<string>Counter</string>
					<add>
						<load>Counter</load>
						<num>1</num>
					</add>
				</save>
				<spawndef>
					<spawnPosition>
						<add>
							<load>SpinVector</load>
							<aimtarget/>
						</add>
					</spawnPosition>
					<spawnDef>Wall</spawnDef>
					<stuffDef>WoodLog</stuffDef>
				</spawndef>
				<spawndef>
					<spawnPosition>
						<add>
							<load>SpinVector</load>
							<aimtarget/>
						</add>
					</spawnPosition>
					<spawnDef>Mote_PlainFlash</spawnDef>
					<scale>4</scale>
				</spawndef>
				<save>
					<string>SpinVector</string>
					<rotate>
						<num>9</num>
						<load>SpinVector</load>
					</rotate>
				</save>
				<delay>1</delay>
			</while>
		</fire>
		<repeatVerb>false</repeatVerb>
		<cooldownTicks>50</cooldownTicks>
		<label>Raise Wall B</label>
		<description>Raise a wall!</description>
		<icon>Things/Item/Equipment/WeaponRanged/MachinePistol</icon>
		<verbProps>
			<verbClass>VerbScript.Verb_Scripted</verbClass>
			<hasStandardCommand>true</hasStandardCommand>
			<warmupTime>0.5</warmupTime>
			<range>19.9</range>
			<soundCast>Shot_MachinePistol</soundCast>
			<soundCastTail>GunTail_Light</soundCastTail>
			<muzzleFlashScale>9</muzzleFlashScale>
			<targetParams>
				<canTargetLocations>true</canTargetLocations>
			</targetParams>
		</verbProps>
	</verbData>
  </VerbScript.VerbScriptDef>
  
  
  <VerbScript.VerbScriptDef>
    <defName>VSC_SimpleTest_CA</defName>
	<verbData>
		<potential>
			<and>
				<hasTrait>Psychopath,0</hasTrait>
			</and>
		</potential>
		<allow>
		</allow>
		<init>
		</init>
		<allowTarget>
		</allowTarget>
		<ai_activeTargets>
			<hostilepawnsinmap></hostilepawnsinmap>
		</ai_activeTargets>
		<ai_targetPoints>
			<rand>
				<num>1</num>
				<num>2</num>
			</rand>
		</ai_targetPoints>
		<ai_targetMode>
		</ai_targetMode>
		<fire>
			<save>
				<string>Direction</string>
				<number>0</number>
			</save>
			<save>
				<string>Position</string>
				<aimtarget/>
			</save>
				
			<while>
				<true/>
				<delay>10</delay>
				<save>
					<string>PassableState</string>
					<number>0</number>
				</save>
				<if>
					<walkable>
						<load>Position</load>
					</walkable>
					<save>
						<string>PassableState</string>
						<number>1</number>
					</save>
				</if>
				
				<if>
					<equal>
						<load>PassableState</load>
						<num>0</num>
					</equal>
					<save>
						<string>Direction</string>
						<modulo>
							<add>
								<load>Direction</load>
								<num>1</num>
							</add>
							<num>4</num>
						</modulo>
					</save>
					<destroyAt>
						<load>Position</load>
					</destroyAt>
					<setTerrain>
						<load>Position</load>
						<terrainDef>FlagstoneMarble</terrainDef>
					</setTerrain>
					<spawndef>
						<load>Position</load>
						<spawnDef>Mote_PlainFlash</spawnDef>
						<scale>4</scale>
					</spawndef>
				</if>
				<if>
					<equal>
						<load>PassableState</load>
						<num>1</num>
					</equal>
					<save>
						<string>Direction</string>
						<modulo>
							<add>
								<load>Direction</load>
								<num>-1</num>
							</add>
							<num>4</num>
						</modulo>
					</save>
					<destroyAt>
						<load>Position</load>
					</destroyAt>
					<!--<setTerrain>
						<load>Position</load>
						<terrainDef>WaterOceanDeep</terrainDef>
					</setTerrain>-->
					<setTerrain>
						<load>Position</load>
						<terrainDef>FlagstoneMarble</terrainDef>
					</setTerrain>
					<spawndef>
						<load>Position</load>
						<spawnDef>Wall</spawnDef>
						<stuffDef>WoodLog</stuffDef>
					</spawndef>
					<spawndef>
						<load>Position</load>
						<spawnDef>Mote_PlainFlash</spawnDef>
						<scale>4</scale>
					</spawndef>
				</if>
				
				<print>
					<equal>
						<load>PassableState</load>
						<num>0</num>
					</equal>
				</print>
				
				<if>
					<equal>
						<load>Direction</load>
						<num>0</num>
					</equal>
					<save>
						<string>Position</string>
						<add>
							<load>Position</load>
							<vector>(0,0,1)</vector>
						</add>
					</save>
				</if>
				<if>
					<equal>
						<load>Direction</load>
						<num>1</num>
					</equal>
					<save>
						<string>Position</string>
						<add>
							<load>Position</load>
							<vector>(1,0,0)</vector>
						</add>
					</save>
				</if>
				<if>
					<equal>
						<load>Direction</load>
						<num>2</num>
					</equal>
					<save>
						<string>Position</string>
						<add>
							<load>Position</load>
							<vector>(0,0,-1)</vector>
						</add>
					</save>
				</if>
				<if>
					<equal>
						<load>Direction</load>
						<num>3</num>
					</equal>
					<save>
						<string>Position</string>
						<add>
							<load>Position</load>
							<vector>(-1,0,0)</vector>
						</add>
					</save>
				</if>
			</while>
		</fire>
		<repeatVerb>false</repeatVerb>
		<cooldownTicks>50</cooldownTicks>
		<label>Langton's Ant</label>
		<description>Anty ant!</description>
		<icon>Things/Item/Equipment/WeaponRanged/MachinePistol</icon>
		<verbProps>
			<verbClass>VerbScript.Verb_Scripted</verbClass>
			<hasStandardCommand>true</hasStandardCommand>
			<warmupTime>0.5</warmupTime>
			<range>45.9</range>
			<soundCast>Shot_MachinePistol</soundCast>
			<soundCastTail>GunTail_Light</soundCastTail>
			<muzzleFlashScale>9</muzzleFlashScale>
			<targetParams>
				<canTargetLocations>true</canTargetLocations>
			</targetParams>
		</verbProps>
	</verbData>
  </VerbScript.VerbScriptDef>
  
