		<fire>
			<save>
				<string>spinnyspinnyVector</string>
				<mult>
					<normalize>
						<sub>
							<aimtarget/>
							<topscope/>
						</sub>
					</normalize>
					<num>5</num>
				</mult>
			</save>
			<save>
				<variableName>countCount</variableName>
				<variable>
					<num>0</num>
				</variable>
			</save>
			<while>
				<less>
					<load>countCount</load>
					<num>100</num>
				</less>
				<delay>10</delay>
				<celldamage>
					<spawnPosition>
						<add>
							<aimtarget/>
							<load>
								<variableName>spinnyspinnyVector</variableName>
							</load>
						</add>
					</spawnPosition>
					<damageType>EMP</damageType>
				</celldamage>
				<save>
					<variableName>countCount</variableName>
					<variable>
						<add>
							<num>1</num>
							<load>
								<variableName>countCount</variableName>
							</load>
						</add>
					</variable>
				</save>
				<save>
					<variableName>spinnyspinnyVector</variableName>
					<variable>
						<rotate>
							<angle>5</angle>
							<vector>
								<load>
									<text>spinnyspinnyVector</text>
								</load>
							</vector>
						</rotate>
					</variable>
				</save>
			</while>
		</fire>
