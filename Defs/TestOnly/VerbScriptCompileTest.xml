				</spawnPosition>
				<spawnDef>Wall</spawnDef>
				<stuffDef>WoodLog</stuffDef>
			</spawndef>
			<spawndef>
				<spawnPosition>
					<add>
						<aimtarget/>
						<vector3>(0,0,-2)</vector3>
					</add>
				</spawnPosition>
				<spawnDef>Wall</spawnDef>
				<stuffDef>WoodLog</stuffDef>
			</spawndef>
			<spawndef>
				<spawnPosition>
					<add>
						<aimtarget/>
						<vector3>(2,0,0)</vector3>
					</add>
				</spawnPosition>
				<spawnDef>Wall</spawnDef>
				<stuffDef>WoodLog</stuffDef>
			</spawndef>
			<spawndef>
				<spawnPosition>
					<add>
						<aimtarget/>
						<vector3>(-2,0,0)</vector3>
